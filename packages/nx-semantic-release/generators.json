{
  "$schema": "http://json-schema.org/schema",
  "name": "nx-semantic-release",
  "version": "0.0.1",
  "generators": {
    "library": {
      "factory": "./src/generators/library/generator",
      "schema": "./src/generators/library/schema.json",
      "aliases": ["lib"],
      "description": "Create a publishable npm library."
    },
    "workflow": {
      "factory": "./src/generators/workflow/generator",
      "schema": "./src/generators/workflow/schema.json",
      "description": "workflow generator"
    }
  }
}
